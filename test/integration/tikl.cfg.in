lotto =
stress = "@LOTTO_PATH@-stress"
record = env LOTTO_RECORD=%x.trace "@LOTTO_PATH@-run"
replay = env LOTTO_REPLAY=%x.trace "@LOTTO_PATH@-run"
inflex = inflex
debug = debug
trace = trace
show = show
check = "@TIKL_CHECK_EXECUTABLE@" %s
BUILD_DIR = "@CMAKE_CURRENT_BINARY_DIR@"
x = %BUILD_DIR/$(basename $(basename %b .c) .cpp)
